{
    "extends": [
        "eslint:recommended", 
        "plugin:import/errors", 
        "plugin:react/recomended",
        "plugin:jsx-a11y",
        "prettier"
    ],
    "plugins": ["react", "import","jsx-a11y"],
    "rules": {
        "react/prop-types": 0,
        "react/react-in-jsx-scope": 0
    },
    "parserOptions":{
        "ecmaVersion": 2022,
        "sourceType": "module",
        "ecmaFeatures":{
            "jsx": true
        }
    },
    "env":{
        "es6":true,
        "browser": true,
        "node":true
    },
    "settings":{
        "react": {
            "version": "detect"
        },
        "import/resolver":{
            "node":{
                "extensions": [".js", ".jsx"]
            }
        }
    }
}